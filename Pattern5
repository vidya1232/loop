public class Pattern5 {

	public static void main(String[] args) {
		int a=0;
		for(int i=1;i<=5;i++)
		{
			int b=5,c=i;
			for(int j=1;j<=5-a;j++)
			{
				if(j==1)
				{
				System.out.print(i+" ");
				}
				else 
				{c=c+b;
					System.out.print(c+" ");
					b--;
				}
				}
			a++;
			System.out.println();
		}
	}
