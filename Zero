class Zero
{
public static void main(String[]args)
{
	int j = 1;
	for(int i=7;i>=1;i--)
	{
		System.out.print("0");
	}
	System.out.println();
	for(int row=1;row<=6;row++)
	{
		for(int col=1;col<=row;col++)
		{	
		System.out.print("0");
	    }
		for(int coll=1;coll<=1;coll++)
		{
		System.out.print(j);
		j++;
		}
		for(int zz=5;zz>=row;zz--)
		{
		System.out.print("0");
		}
		System.out.println();
	}
	
}
}
/*
0000000
0100000
0020000
0003000
0000400
0000050
0000006
*/
