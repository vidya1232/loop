class Pattern1
{
public static void main(String[]args)
{
	int n=6;
	int m=2;
	for(int j=1;j<=8;j++)
		{
			System.out.print(1);
		}
		System.out.println();
	for(int i=1;i<=7;i++)
	{
		for(int j=1;j<=n;j++)
		{
			System.out.print(1);
		}
		n--;
		for(int ji=m;ji>=1;ji--)
		{
			System.out.print(m);
		}
		m++;
		System.out.println();
	}
}
}
